<!DOCTYPE html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title>MASUK | <%=data.botUsername%></title>
  <!-- base:css -->
  <link rel="stylesheet" href="vendors/mdi/css/materialdesignicons.min.css">
  <link rel="stylesheet" href="vendors/feather/feather.css">
  <link rel="stylesheet" href="vendors/base/vendor.bundle.base.css">
  <!-- endinject -->
  <!-- plugin css for this page -->
  <link rel="stylesheet" href="vendors/fontawesome/css/all.min.css">
  <!-- End plugin css for this page -->
  <!-- inject:css -->
  <link rel="stylesheet" href="css/style.css">
  <!-- endinject -->
  <link rel="shortcut icon" href="<%=data.botAvatar%>" />
</head>

<body>
  <div class="container-scroller">
    <div class="container-fluid page-body-wrapper full-page-wrapper">
      <div class="content-wrapper d-flex align-items-center auth lock-full-bg">
        <div class="row w-100">
          <div class="col-md-4 mx-auto">
            <div class="auth-form-transparent text-left p-5 text-center w-100">
              <img src="<%=data.botAvatar%>" class="lock-profile-img" alt="img">
              <form action="/login" method="post" class="form pt-5">
                <%-data.message%>
                <div class="form-group">
                  <label for="username" class="text-left text-white">NAMA PENGGUNA</label>
                  <input type="text" class="form-control text-center text-white" id="username" name="username" placeholder="Nama Pengguna" required="">
                </div>
                <div class="form-group">
                  <label for="password" class="text-left text-white">KATA SANDI</label>
                  <input type="password" class="form-control text-center text-white" id="password" name="password" placeholder="Masukan Password" required="">
                </div>
                <div class="mt-5">
                  <button type="submit" class="btn btn-block btn-secondary  btn-lg font-weight-medium">Masuk</button>
                </div>
                <div class="mt-3 text-center">
                  <a href="https://discord.gg/imutserver">FIND ON DISCORD</a>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
      <!-- content-wrapper ends -->
    </div>
    <!-- page-body-wrapper ends -->
  </div>
  <!-- container-scroller -->
  <!-- base:js -->
  <script src="vendors/base/vendor.bundle.base.js"></script>
  <!-- endinject -->
  <!-- inject:js -->
  <script src="js/off-canvas.js"></script>
  <script src="js/hoverable-collapse.js"></script>
  <script src="js/template.js"></script>
  <!-- endinject -->
</body>

</html>